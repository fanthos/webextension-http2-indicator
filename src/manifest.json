{
    "name": "HTTP/2 and SPDY indicator"
  , "description": "An indicator showing page is loaded using SPDY or HTML/2."
  , "version": "0.1.0"
  , "manifest_version": 2
  , "permissions": ["tabs", "webRequest", "webNavigation", "<all_urls>"]
  , "background": {
        "persistent": false
      , "scripts": [
          "background.js"
        ]
    }
  , "icons": {
        "48": "imgs/icon48.png"
      , "128": "imgs/icon.png"
    }
}
